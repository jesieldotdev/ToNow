{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo/config-plugins": "~9.0.0",
    "@expo/prebuild-config": "~8.0.0",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "@reduxjs/toolkit": "^2.6.0",
    "@types/lodash": "^4.17.16",
    "axios": "^1.8.1",
    "expo": "^52.0.37",
    "expo-device": "~7.0.2",
    "expo-navigation-bar": "~4.0.8",
    "expo-notifications": "~0.29.13",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "^0.27.2",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.477.0",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.3",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-calendars": "^1.1310.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-paper": "^5.13.1",
    "react-native-paper-dates": "^0.22.34",
    "react-native-picker-select": "^9.3.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "^15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-wheel-picker-expo": "^0.5.4",
    "react-redux": "^9.2.0",
    "redux-deep-persist": "^1.0.7",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.0"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "redux-persist",
          "redux-persist-expo-filesystem",
          "redux-deep-persist",
          "postcss",
          "lodash",
          "@expo/config-plugins",
          "@expo/prebuild-config"
        ]
      }
    }
  }
}
